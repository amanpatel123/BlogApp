
<h1>Blog Posts</h1>

<% @posts.each do |post| %>
    <div class= "card card-inverse card-primary mb-4 bg-light">
    <div class="card-body ">
        <h3><%= post.title %></h3>
        <p><%= post.body %></p>
        <%= link_to "Read More", post_path(post), :class => 'btn btn-outline-secondary' %>
    </div>
    </div>

   
<% end %>